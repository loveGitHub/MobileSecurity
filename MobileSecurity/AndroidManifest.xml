    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mobilesecurity.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mobilesecurity.activities.RegisterActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.HomeActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.ForgotPassActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.MyKeyStatus" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.ChangePassActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.HiddenModeActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.ChangeHeroActivity" >
        </activity>
        <activity android:name="net.bgreco.DirectoryPicker" />
        <activity android:name="com.mobilesecurity.activities.LockScreenPreference" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.GridActivity" >
        </activity>
        <activity android:name="com.blahti.example.drag3.DragActivity" >
        </activity>
        <activity android:name="com.mobilesecurity.activities.AppUnderProtection" >
        </activity>
        
        <activity android:name="com.mobilesecurity.activities.TwoScreenLock" >
        </activity>
        
        
    </application>

</manifest>